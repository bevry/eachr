{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "maxNodeModuleJsDepth": 5,
    "moduleResolution": "Node",
    "strict": true,
    "target": "ES2022",
    "skipLibCheck": false,
    "module": "ESNext",
    "downlevelIteration": true
  },
  "include": ["source"]
}
